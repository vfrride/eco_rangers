= form_for @ranger_icon do |f|
  - if @ranger_icon.errors.any?
    #error_explanation
      %h2= "#{pluralize(@ranger_icon.errors.count, "error")} prohibited this ranger_icon from being saved:"
      %ul
        - @ranger_icon.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :image_url
    = f.text_field :image_url
  .actions
    = f.submit 'Save'
